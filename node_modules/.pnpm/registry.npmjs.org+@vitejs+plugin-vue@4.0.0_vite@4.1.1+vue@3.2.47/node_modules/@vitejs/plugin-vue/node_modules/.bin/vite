#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/repos/Vue3-Treeee/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/repos/Vue3-Treeee/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmjs.org+vite@4.1.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmjs.org+vite@4.1.1/node_modules/vite/bin/vite.js" "$@"
fi
